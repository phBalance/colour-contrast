{
  "name": "@phbalance/contrast-colour",
  "version": "0.0.4",
  "description": "Small library for calculating contrasting colours.",
  "main": "colour.js",
  "module": "colour.es6.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c --environment BUILD:development",
    "clean": "rimraf colour.js colour.js.map colour.es6.js colour.es6.js.map colour.d.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"No tests specified\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phBalance/colour-contrast.git"
  },
  "bugs": {
    "url": "https://github.com/phBalance/colour-contrast/issues"
  },
  "keywords": [
    "Colour",
    "Color",
    "Contrast",
    "High Contrast"
  ],
  "author": "Peter Hunnisett <peter@softwarebalm.com> (https://softwarebalm.com)",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/phBalance/colour-contrast#readme",
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "color": "^3.1.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "np": "^6.4.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-loader": "^5.4.5",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "color": ">=1.0.0"
  }
}
