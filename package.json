{
  "name": "@phbalance/contrast-colour",
  "version": "0.0.6",
  "description": "Small library for calculating contrasting colours.",
  "main": "colour.js",
  "module": "colour.es6.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c --environment BUILD:development",
    "clean": "rimraf colour.js colour.js.map colour.es6.js colour.es6.js.map colour.d.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"No tests specified\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phBalance/colour-contrast.git"
  },
  "bugs": {
    "url": "https://github.com/phBalance/colour-contrast/issues"
  },
  "keywords": [
    "Colour",
    "Color",
    "Contrast",
    "High Contrast"
  ],
  "author": "Peter Hunnisett <peter@softwarebalm.com> (https://softwarebalm.com)",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/phBalance/colour-contrast#readme",
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "color": "^3.1.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "np": "^8.0.4",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "color": ">=1.0.0"
  }
}
